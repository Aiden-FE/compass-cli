{
  "name": "@compass-aiden/cli",
  "type": "module",
  "version": "0.0.3",
  "description": "Command line interfaces for Compass CLI",
  "main": "index.js",
  "bin": {
    "compass": "index.js"
  },
  "scripts": {
    "dev": "rollup -w -c rollup.config.js",
    "build": "npm run clean && rollup -c rollup.config.js",
    "lint": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src",
    "prepare": "npx simple-git-hooks",
    "clean": "rimraf dist && rimraf types && rimraf stats.html"
  },
  "simple-git-hooks": {
    "pre-commit": "npx pretty-quick --staged",
    "commit-msg": "npx --no -- commitlint --edit $1"
  },
  "files": [
    "dist",
    "types",
    "index.js"
  ],
  "keywords": [
    "commander",
    "pull template",
    "template plugins",
    "code snippet",
    "custom template"
  ],
  "author": "Aiden_FE@outlook.com",
  "repository": "https://github.com/Aiden-FE/compass-cli",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@compass-aiden/eslint-config": "^1.6.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.27",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-summary": "^2.0.0",
    "rollup-plugin-ts": "^3.4.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.10.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@compass-aiden/helpers": "^0.1.1",
    "@compass-aiden/telegram": "^2.1.0",
    "axios": "^1.6.7",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.16",
    "ora": "^8.0.1"
  }
}
